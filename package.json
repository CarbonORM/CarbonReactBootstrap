{
  "name": "reactboot",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.40",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "bootstrap": "^5.3.1",
    "classnames": "^2.3.2",
    "concurrently": "^8.2.0",
    "css-modules-typescript-loader": "^4.0.1",
    "customize-cra": "^1.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "react": "^18.2.0",
    "react-app-rewire-typings-for-css-module": "^4.0.3",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.54.5",
    "typescript": "^4.9.5",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-log": "^3.0.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "npm cache clean --force && npm run build:css && concurrently \"npm:watch:sass\" \"npm:rewired:start\"",
    "rewired:start": "react-app-rewired start  --experimental-modules",
    "build:css": "sass ./src:./src && tcm src",
    "build": "npm run build:css && PUBLIC_URL=https://drop-in-gaming.github.io/node-core/ react-app-rewired build",
    "test": "react-app-rewired test",
    "watch:sass": "sass ./src:./src --watch",
    "eject": "react-app-rewired eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
